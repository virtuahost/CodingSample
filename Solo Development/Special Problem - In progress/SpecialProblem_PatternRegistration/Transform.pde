public class lstSqrTransf{
  public float scaleVal;
  public float cosVal;
  public float sinVal;
  public float tranValX;
  public float tranValY; 
  
  public lstSqrTransf(float s, float c, float sn, float x, float y)
  {
    scaleVal = s;
    cosVal = c;
    sinVal = sn;
    tranValX = x;
    tranValY = y; 
  }
}
